<div class="span2"></div>
<div class="span8">	
	<h1>Admins</h1>
	<%= link_to "Create admin", new_admin_user_path, :class=>"btn btn-inverse" %>
	<%= link_to "Back", admin_home_path, :class=>"btn btn-inverse" %>
	
	<table id="server-table" class="table table-condensed span12">
		<th>Name</th>
		<th></th>
		<th></th>
		<% @users.each do |user| %>
			<tr>
				<td><%= user.username %></td>
				<td><%#= user.email %></td>
				<td>
					<div class="input-append">
					<div class="btn-group">
						<button class="btn dropdown-toggle btn-inverse" data-toggle="dropdown">
							Action
							<span class="caret"></span>
						</button>
						<ul class="dropdown-menu">
							<li><%= link_to "Edit", edit_admin_user_path(user.id)%></li>
							<li><%= link_to 'Destroy', admin_user_path(user.id), confirm: 'Are you sure?', :method => :delete %></li>
					</div>
				</div>
				</td>
			</tr>
		<% end %>
	</table>
</div>
<div class="span2"></div>